{
  "version": 2,
  "builds": [
    {
      "use": "@vercel/node",
      "src": "api/**/*.js"
    },
    {
      "use": "@vercel/jekyll",
      "src": "Gemfile"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
